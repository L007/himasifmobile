<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.brianr.himasifmobile.OtherFragment">
    <Button
        android:id="@+id/visimisi"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="50dp"
        android:layout_marginLeft="15dp"
        android:background="@drawable/circle_button"
        android:paddingTop="25dp"
        android:textColor="#FFF"
        android:text="VISI MISI"
        android:translationZ="10dp"
        android:textSize="12dp"
        android:drawableTint="#FFF"/>
    <Button
        android:id="@+id/tentang_kami"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/circle_button"
        android:layout_marginTop="50dp"
        android:layout_marginRight="15dp"
        android:layout_alignParentRight="true"
        android:drawableTint="#FFF"
        android:translationZ="10dp"
        android:paddingTop="25dp"
        android:textColor="#FFF"
        android:textSize="12dp"
        android:text="TENTANG \nKAMI"/>
    <Button
        android:id="@+id/pengurus"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginLeft="15dp"
        android:background="@drawable/circle_button"
        android:layout_marginTop="220dp"
        android:drawableTint="#FFF"
        android:translationZ="10dp"
        android:paddingTop="25dp"
        android:textColor="#FFF"
        android:textSize="12dp"
        android:text="PENGURUS \nHIMASIF"/>
    <Button
        android:id="@+id/aspirasi"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_alignParentRight="true"
        android:layout_marginTop="220dp"
        android:layout_marginRight="15dp"
        android:background="@drawable/circle_button"
        android:drawableTint="#FFF"
        android:translationZ="10dp"
        android:paddingTop="25dp"
        android:textColor="#FFF"
        android:textSize="12dp"
        android:text="ASPIRASI"/>

</RelativeLayout>
